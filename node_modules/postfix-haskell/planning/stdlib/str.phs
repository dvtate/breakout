"./prelude.phs" include use

# String TypeClass
{ I32 I32 } pack class $Str =
{ type Str == } { unpack pop } $len fun
{ type Str == } { unpack swap pop } $addr fun

# Overload as operator to convert string literals
{ { $lit $t } = t type Str == } {
    $lit =
    { lit } pack Str make
} $as global fun

{
    $lit =
    { lit } pack Str make
} $Str.new =